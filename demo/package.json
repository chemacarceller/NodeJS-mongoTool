{
  "name": "demo_mongoTool",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:macos": "electron-builder --macos --dir",
    "dist:macos": "electron-builder --macos",
